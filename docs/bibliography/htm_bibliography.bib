journal = {Frontiers in neural circuits},
keywords = {Animals,Cerebral Cortex,Cerebral Cortex: physiology,Dendrites,Dendrites: physiology,Mice,Microscopy, Electron,Microscopy, Electron: methods,Microscopy, Fluorescence,Microscopy, Fluorescence: methods,Neural Pathways,Neural Pathways: physiology,Pyramidal Cells,Pyramidal Cells: physiology,Pyramidal neurons,Synapses,Synapses: physiology,Thalamus,Thalamus: physiology,Tomography,Tomography: methods},
