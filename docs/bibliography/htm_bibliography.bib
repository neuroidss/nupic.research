file = {::},
isbn = {1529-2401 (Electronic)$\backslash$r0270-6474 (Linking)},
issn = {1529-2401},
journal = {The Journal of neuroscience : the official journal of the Society for Neuroscience},
keywords = {Acoustic Stimulation,Animals,Attention,Attention: physiology,Auditory,Auditory Cortex,Auditory Cortex: physiology,Auditory Perception,Auditory Perception: physiology,Auditory: physiology,Electroencephalography,Evoked Potentials,Macaca fascicularis,Male,Neurons,Neurons: physiology,Sequence Learning},
