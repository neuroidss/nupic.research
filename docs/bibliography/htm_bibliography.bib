pages = {8789--98},
title = {{Dendritic calcium spike initiation and repolarization are controlled by distinct potassium channel subtypes in CA1 pyramidal neurons.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/10516298},
volume = {19},
year = {1999}
}
@article{Gordon2006,
abstract = {Synaptic plasticity rules widely determine how cortical networks develop and store information. Using confocal imaging and dual site focal synaptic stimulation, we show that basal dendrites, which receive the majority of synapses innervating neocortical pyramidal neurons, contain two compartments with respect to plasticity rules. Synapses innervating the proximal basal tree are easily modified when paired with the global activity of the neuron. In contrast, synapses innervating the distal basal tree fail to change in response to global suprathreshold activity or local dendritic spikes. These synapses can undergo long-term potentiation under unusual conditions when local NMDA spikes, which evoke large calcium transients, are paired with a "gating molecule," BDNF. Moreover, these synapses use a new temporal plasticity rule, which is an order of magnitude longer than spike timing dependent plasticity and prefers reversed presynaptic/postsynaptic activation order. The newly described plasticity compartmentalization of basal dendrites expands the networks plasticity rules and may support different learning and developmental functions.},
annote = {This paper studies synaptic plasticity rules of proximal and distal synapses in the basal dendrites. The main findings are

1. Proximal synapses can be easily modified when EPSP is paired with action potentials at the cell body, following classical STDP rule

2. Distal synapse don't change when paired with either action potentials or NMDA spikes alone.

3. Pairing NMDA spike with a growth factor, BDNF cause LTP of distal synapses. It is not yet known when BDNF is regulated in the brain though.

4. At this condition of 3, plasticity can be triggered even if EPSP is delayed relative to NMDA spike by more than 100 ms, which is much longer than the classical STDP.},
author = {Gordon, Urit and Polsky, Alon and Schiller, Jackie},
issn = {1529-2401},
journal = {The Journal of neuroscience : the official journal of the Society for Neuroscience},
keywords = {Action Potentials,Action Potentials: physiology,Animals,Calcium Signaling,Calcium Signaling: physiology,Dendrites,Dendrites: physiology,Female,Male,Neocortex,Neocortex: cytology,Neocortex: physiology,Neuronal Plasticity,Neuronal Plasticity: physiology,Plasticity,Pyramidal Cells,Pyramidal Cells: cytology,Pyramidal Cells: physiology,Pyramidal neurons,Rats,Rats, Wistar},
mendeley-tags = {Dendrites,Plasticity,Pyramidal neurons},
month = {dec},
number = {49},
pages = {12717--26},
title = {{Plasticity compartments in basal dendrites of neocortical pyramidal neurons.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17151275},
