issn = {0021-9940},
journal = {Journal of comparative and physiological psychology},
keywords = {ANIMALS,CATS,EXPERIMENTAL LAB STUDY,MOVEMENT,NEWBORN,Sensorimotor Inference,VISUAL PERCEPTION},
mendeley-tags = {Sensorimotor Inference},
