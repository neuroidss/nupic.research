issn = {1529-2401},
journal = {The Journal of neuroscience},
keywords = {Sequence Learning,and sleep,dendritic spikes,during phases of rest,generates intermittent patterns of,learning,memory,memory center,network,of the brain,replay,ripples,sharp-wave,significance statement,strongly increased overall activity,the,the hippocampus,the so-called sharp-wave,we investigate their,with high-frequency oscillations},
mendeley-tags = {Sequence Learning},
number = {49},
pages = {16236--58},
pmid = {26658873},
title = {{A Unified Dynamic Model for Learning, Replay, and Sharp-Wave/Ripples.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26658873},
volume = {35},
year = {2015}
